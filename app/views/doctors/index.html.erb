<h1>Listing doctors</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Fax number</th>
      <th>Npi</th>
      <th>City</th>
      <th>Contact name</th>
      <th>Region</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @doctors.each do |doctor| %>
      <tr>
        <td><%= doctor.first_name %></td>
        <td><%= doctor.last_name %></td>
        <td><%= doctor.fax_number %></td>
        <td><%= doctor.npi %></td>
        <td><%= doctor.city %></td>
        <td><%= doctor.contact_name %></td>
        <td><%= doctor.region_id %></td>
        <td><%= doctor.notes %></td>
        <td><%= link_to 'Show', doctor %></td>
        <td><%= link_to 'Edit', edit_doctor_path(doctor) %></td>
        <td><%= link_to 'Destroy', doctor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Doctor', new_doctor_path %>
